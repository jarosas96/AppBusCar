<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragments.ManueAdminFragment">

    <androidx.cardview.widget.CardView
        android:id="@+id/cvRegistroFlota"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:layout_marginTop="21dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cvRegistroPasajero">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ticketImgFlota"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:layout_gravity="start|center_vertical"
                android:layout_weight="0.2"
                android:src="@drawable/ticketimg" />

            <TextView
                android:id="@+id/lblRegistroFlota"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="@string/lblRegistroFlota"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/playIconFlota"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical|end"
                android:layout_weight="0.2"
                android:src="@drawable/playicon"
                app:srcCompat="@drawable/playicon"
                tools:srcCompat="@drawable/playicon" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvRegistroBoletos"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:layout_marginTop="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cvRegistroFlota">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ticketImgBoleto"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:layout_gravity="start|center_vertical"
                android:layout_marginStart="10dp"
                android:layout_weight="0.4"
                android:src="@drawable/ticketimg" />

            <TextView
                android:id="@+id/lblRegistroBoleto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="@string/lblRegistroBoletos"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/playIconBoleto"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical|end"
                android:layout_weight="0.2"
                android:src="@drawable/playicon"
                app:srcCompat="@drawable/playicon"
                tools:srcCompat="@drawable/playicon" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvRegistroViaje"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:layout_marginTop="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cvRegistroBoletos">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ticketImgViaje"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:layout_gravity="start|center_vertical"
                android:layout_weight="0.2"
                android:src="@drawable/ticketimg" />

            <TextView
                android:id="@+id/lblRegistroViaje"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|center_vertical"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="@string/lblViaje"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/playIconViaje"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical|end"
                android:layout_weight="0.2"
                android:src="@drawable/playicon"
                app:srcCompat="@drawable/playicon"
                tools:srcCompat="@drawable/playicon" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvConsultaBoleto"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:layout_marginTop="23dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cvRegistroViaje">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ticketImgConBoleto"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:layout_gravity="start|center_vertical"
                android:layout_weight="0.2"
                android:src="@drawable/ticketimg" />

            <TextView
                android:id="@+id/lblConsultaBoletos"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center|center_vertical"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="@string/lblConsultaBoletos"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/playIconConBoleto"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical|end"
                android:layout_weight="0.2"
                android:src="@drawable/playicon"
                app:srcCompat="@drawable/playicon"
                tools:srcCompat="@drawable/playicon" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardViewUserPasajero"
        android:layout_width="392dp"
        android:layout_height="73dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="73dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start|center_vertical"
                android:layout_weight="1"
                android:src="@drawable/micuenta" />

            <LinearLayout
                android:layout_width="19dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/lblName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="bottom|start"
                    android:text="Xavi" />

                <TextView
                    android:id="@+id/lvlVerPerfil"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start|center_vertical"
                    android:text="@string/lblVerPeril"
                    android:textColor="#FFC096C8" />
            </LinearLayout>

            <TextView
                android:id="@+id/lvlCerrarSesion"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|start|center_vertical"
                android:layout_marginBottom="10.5dp"
                android:layout_weight="1"
                android:gravity="bottom|start"
                android:text="@string/lblCerrarSesion"
                android:textColor="#C096C8" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvRegistroPasajero"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:layout_marginTop="22dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cvRegistroRutas">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ticketImgPasajero"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:layout_gravity="start|center_vertical"
                android:layout_weight="0.2"
                android:src="@drawable/ticketimg" />

            <TextView
                android:id="@+id/lblRegistroPasajero"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="@string/lblRegistroPasajero"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/playIconPasajero"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical|end"
                android:layout_weight="0.2"
                android:src="@drawable/playicon"
                app:srcCompat="@drawable/playicon"
                tools:srcCompat="@drawable/playicon" />

        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cvRegistroRutas"
        android:layout_width="380dp"
        android:layout_height="60dp"
        android:layout_marginTop="19dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardViewUserPasajero">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ticketImg"
                android:layout_width="40dp"
                android:layout_height="30dp"
                android:layout_gravity="start|center_vertical"
                android:layout_weight="0.2"
                android:src="@drawable/ticketimg" />

            <TextView
                android:id="@+id/lblRegistroRutas"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="@string/lblRegistroRutas"
                android:textSize="20sp" />

            <ImageView
                android:id="@+id/playIcon"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_gravity="center_vertical|end"
                android:layout_weight="0.2"
                android:src="@drawable/playicon"
                app:srcCompat="@drawable/playicon"
                tools:srcCompat="@drawable/playicon" />

        </LinearLayout>

    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>