<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PassengerInfoActivity">

    <RelativeLayout
        android:id="@+id/ctnPassengerInfo"
        android:layout_width="410dp"
        android:layout_height="90dp"
        android:layout_marginStart="1dp"
        android:layout_marginEnd="1dp"
        android:background="#2E2929"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">

        <TextView
            android:id="@+id/lblPassengerInfo"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="@string/lblPassengerInfo"
            android:textAppearance="@style/TextAppearance.AppCompat.Display1"
            android:textColor="#FFFFFF" />
    </RelativeLayout>

    <TextView
        android:id="@+id/lblBoletoViaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="289dp"
        android:text="@string/lblBoletoViaje"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ctnPassengerInfo" />

    <TextView
        android:id="@+id/lblOrigenViaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="23dp"
        android:text="@string/lblOrigenViaje"
        app:layout_constraintEnd_toStartOf="@+id/txtOrigenDestino"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblBoletoViaje" />

    <TextView
        android:id="@+id/lblDestinoViaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="72dp"
        android:text="@string/lblDestinoViaje"
        app:layout_constraintEnd_toStartOf="@+id/txtDestinoViaje"
        app:layout_constraintTop_toBottomOf="@+id/ctnPassengerInfo" />

    <EditText
        android:id="@+id/txtOrigenDestino"
        android:layout_width="118dp"
        android:layout_height="37dp"
        android:layout_marginTop="18dp"
        android:layout_marginEnd="15dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toStartOf="@+id/lblDestinoViaje"
        app:layout_constraintTop_toBottomOf="@+id/lblBoletoViaje" />

    <EditText
        android:id="@+id/txtDestinoViaje"
        android:layout_width="133dp"
        android:layout_height="38dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="19dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ctnPassengerInfo" />

    <EditText
        android:id="@+id/txtNombresInfo"
        android:layout_width="366dp"
        android:layout_height="45dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="18dp"
        android:ems="10"
        android:hint="@string/txtNombres"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ctnPassengerInfo" />

    <EditText
        android:id="@+id/txtApellidoPInfo"
        android:layout_width="367dp"
        android:layout_height="41dp"
        android:layout_marginStart="27dp"
        android:layout_marginEnd="17dp"
        android:ems="10"
        android:hint="@string/txtApellidoP"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNombresInfo" />

    <EditText
        android:id="@+id/txtApellidoMInfo"
        android:layout_width="369dp"
        android:layout_height="46dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="14dp"
        android:ems="10"
        android:hint="@string/txtApellidoM"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtApellidoPInfo" />

    <TextView
        android:id="@+id/lblNasiento"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="49dp"
        android:text="@string/lblNasiento"
        app:layout_constraintEnd_toStartOf="@+id/txtNasiento"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtApellidoMInfo" />

    <EditText
        android:id="@+id/txtNasiento"
        android:layout_width="223dp"
        android:layout_height="42dp"
        android:layout_marginEnd="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtApellidoMInfo" />

    <TextView
        android:id="@+id/lblHoraSalida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="24dp"
        android:text="@string/lblHoraSalida"
        app:layout_constraintEnd_toStartOf="@+id/txtHoraSalida"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblNasiento" />

    <EditText
        android:id="@+id/txtHoraSalida"
        android:layout_width="221dp"
        android:layout_height="44dp"
        android:layout_marginEnd="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNasiento" />

    <TextView
        android:id="@+id/lblFechaSalida"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="31dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="16dp"
        android:text="@string/lblFechaSalida"
        app:layout_constraintEnd_toStartOf="@+id/txtFechasSalida"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblHoraSalida" />

    <EditText
        android:id="@+id/txtFechasSalida"
        android:layout_width="221dp"
        android:layout_height="49dp"
        android:layout_marginEnd="38dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtHoraSalida" />

    <Button
        android:id="@+id/btnSalirInfo"
        android:layout_width="358dp"
        android:layout_height="52dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="37dp"
        android:layout_marginEnd="16dp"
        android:backgroundTint="#11615A"
        android:text="@string/btnSalir"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtFechasSalida" />

    <ImageButton
        android:id="@+id/imgBtnMiCuenta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="33dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSalirInfo"
        app:srcCompat="@drawable/micuenta" />

    <ImageButton
        android:id="@+id/imgBtnViaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginTop="59dp"
        android:layout_marginEnd="204dp"
        app:layout_constraintEnd_toStartOf="@+id/imgBtnMiCuenta"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnSalirInfo"
        app:srcCompat="@drawable/viaje" />

    <TextView
        android:id="@+id/lblViajePassengerRegistry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="220dp"
        android:text="@string/lblViaje"
        app:layout_constraintEnd_toStartOf="@+id/lblCuentaPassengerRegistry"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgBtnViaje" />

    <TextView
        android:id="@+id/lblCuentaPassengerRegistry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="46dp"
        android:text="@string/lblMiCuenta"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgBtnMiCuenta" />

</androidx.constraintlayout.widget.ConstraintLayout>